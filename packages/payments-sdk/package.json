{
  "name": "@bloque/payments-sdk",
  "version": "0.0.1",
  "description": "Official Bloque SDK for creating and managing payments, checkouts, and transactions.",
  "type": "module",
  "keywords": [
    "bloque",
    "payments",
    "sdk",
    "api"
  ],
  "author": "Nestor Cortina <nexckycort@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://github.com/bloque-app/bloque-payments#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloque-app/bloque-payments",
    "directory": "packages/payments-sdk"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "clean": "rm -rf node_modules",
    "check": "biome check --write",
    "typecheck": "tsgo --noEmit"
  },
  "devDependencies": {
    "@rslib/core": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:"
  }
}
